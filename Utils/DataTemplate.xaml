<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:vm ="clr-namespace:AuthoWorkAppp.ViewModel"
                    xmlns:view ="clr-namespace:AuthoWorkAppp.View">

    <DataTemplate DataType="{x:Type vm:AdminViewModel}">
        <view:Admin/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:PersonalViewModel}">
        <view:Personal/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:PlannerViewModel}">
        <view:Planner/>
    </DataTemplate>
</ResourceDictionary>